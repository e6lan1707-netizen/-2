# Сложение
a = 7 + 5
print("Сложение: 7 + 5 =", a)

# Вычитание
b = 10 - 3
print("Вычитание: 10 - 3 =", b)

# Умножение
c = 4 * 6
print("Умножение: 4 * 6 =", c)

# Возведение в степень
d = 2 ** 5
print("Возведение в степень: 2 ** 5 =", d)

# Деление
e = 15 / 4
print("Деление: 15 / 4 =", e)

# Целочисленное деление
f = 15 // 4
print("Целочисленное деление: 15 // 4 =", f)

# Остаток от деления
g = 15 % 4
print("Остаток от деления: 15 % 4 =", g)
